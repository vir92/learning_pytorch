        "print(\"Device\" ,device)\n",
        "# Create two random tensors on GPU\n",
        "random_tensor_1 = torch.rand(2,3, device = device)\n",
        "random_tensor_2 = torch.rand(2,3, device = device)\n",
        "print(random_tensor_1)\n",
        "print(random_tensor_2)"
